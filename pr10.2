name = raw_input("Enter file:")
if len(name) < 1 : name = "mbox-short.txt"
handle = open(name)
day = dict()

for line in handle:
    if not line.startswith('From '): continue
    words = line.split()
    time = words[5]
    hour = time.split(':')[0]
    day[hour] = day.get(hour,0) + 1
    

for h,c in sorted(day.items()):
    print h,c
